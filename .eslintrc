{
  "extends": ["prettier", "airbnb-base"],
  "parser": "babel-eslint",
  "plugins": ["babel"],
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "import/no-extraneous-dependencies": [
      "error",
      { 
        "devDependencies": ["webpack.*.js"] 
      }
    ],
    "no-console": "off", // TODO set to true on production,
    "no-unused-vars": "off"
  }
}
